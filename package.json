{
    "name": "data-translate",
    "description": "A sample plugin that shows how to access a layer inside a Data plugin",
    "version": "0.1.1",
    "engines": {
        "sketch": ">=3.0"
    },
    "skpm": {
        "name": "data-translate",
        "manifest": "src/manifest.json",
        "main": "data-translate.sketchplugin",
        "assets": [
            "assets/**/*"
        ]
    },
    "scripts": {
        "build": "skpm-build",
        "watch": "skpm-build --watch",
        "start": "skpm-build --watch --run",
        "postinstall": "npm run build && skpm-link"
    },
    "devDependencies": {
        "@skpm/builder": "^0.5.10"
    },
    "author": "Bohemian Coding",
    "repository": "https://github.com/BohemianCoding/plugins.example.data-translate.git",
    "dependencies": {
        "@skpm/fs": "^0.2.0"
    }
}
